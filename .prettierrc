# Options: https://prettier.io/docs/en/options.html
tabWidth: 2
useTabs: false
trailingComma: 'all'
semi: false
singleQuote: true
endOfLine: 'lf'
bracketSpacing: true
arrowParens: 'avoid'
printWidth: 60
htmlWhitespaceSensitivity: 'css'
proseWrap: 'preserve'
jsxBracketSameLine: false
jsxSingleQuote: false
quoteProps: 'as-needed'
plugins: []
